["Algeth'ar Academy "] = {
["iconSource"] = -1,
["wagoID"] = "LyVvlKxvs",
["parent"] = "Spell CDs on Nameplate - (Season 4) DF",
["preferToUpdate"] = false,
["customText"] = "",
["yOffset"] = 0,
["anchorPoint"] = "CENTER",
["cooldownSwipe"] = true,
["url"] = "https://wago.io/LyVvlKxvs/10",
["actions"] = {
