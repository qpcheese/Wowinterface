       }\n        }\n        \n        -- If MRT is not enabled, don't do anything.\n        if not (IsAddOnLoaded(\"ExRT\") or IsAddOnLoaded(\"MRT\")) then return end\n        \n        -- Colors player names (and nicknames) according to their class\n        for unit in WA_IterateGroupMembers() do\n            local name = UnitName(unit)\n            \n            aura_env.gsubAutoColorData[name] = WA_ClassColorName(unit)\n            \n            -- Color nicknames according to the class they are currently playing\n            if LiquidAPI then\n                local nickname = LiquidAPI:GetName(name)\n                \n                if nickname and nickname ~= name then\n                    local class = UnitClassBase(unit)\n                    aura_env.gsubAutoColorData[nickname] = string.format(\"|c%s%s|r\", RAID_CLASS_COLORS[class].colorStr, nickname)\n                end\n            end\n        end\n        \n        local notes = {\n            aura_env.config.enableSharedNote and _G.VExRT.Note.Text1 or nil,\n            aura_env.config.enablePersonalNote and _G.VExRT.Note.SelfText or nil\n        }\n        \n        local playerName = UnitName(\"player\")\n        local playerNickname = LiquidAPI and LiquidAPI:GetName(\"player\") or playerName\n        local playerSpecID, _, _, _, playerRole = GetSpecializationInfo(GetSpecialization())\n        local playerSpecType = aura_env.specToType[playerSpecID]\n        local playerGroup = UnitInRaid(\"player\") and GetRaidRosterInfo(UnitInRaid(\"player\")) or 1\n        \n        for _, note in pairs(notes) do\n            for line in note:gmatch(\"[^\\r\\n]+\") do\n                local myReminders = {}\n                local options, reminders = line:match(\"^{(.-)}%d+:%d+%s%-%s.-%s%-%s(.+)\")\n                \n                -- If the note is not showing a static timer, the above pattern match will have failed\n                -- Pattern match again, assuming there is no static timer shown\n                if not reminders then\n                    options, reminders = line:match(\"^{(.-)}.-%s%-%s(.+)\")\n                end\n                \n                -- First check which reminders on this line are relevant to the player\n                -- If they are relevant, save them in the myReminders table so we can queue them later on\n                if reminders then\n                    for reminder in reminders:gmatch(\"(.-)%s%s\") do\n                        local target, text = reminder:match(\"(.-)%s({.+})\")\n                        \n                        if target and text then\n                            target = target:match(\"||c%x%x%x%x%x%x%x%x(.-)||r\") or target -- Remove name coloring (if any)\n                            text = text:match(\"{[Tt][Ee][Xx][Tt]}(.-){/[Tt][Ee][Xx][Tt]}\") or text -- Remove {text} tags (if any)\n                            local role = target:upper():match(\"{ROLE:(.-)}\")\n                            local group = target:lower():match(\"group:(%d)+\")\n                            \n                            if target:lower() == \"{everyone}\" then -- Reminders for everyone\n                                table.insert(myReminders, text)\n                            elseif role and role == playerRole then -- Reminders for a specific role (tank, healer, damager)\n                                table.insert(myReminders, text)\n                            elseif group and tonumber(group) == playerGroup then -- Reminders for a specific raid subgroup\n                                table.insert(myReminders, text)\n                            elseif target == playerSpecType then -- Reminders for a specific spec type (melee, ranged)\n                                table.insert(myReminders, text)\n                            elseif target == playerName or target == playerNickname then -- Reminders for a specific player (matches both character name and nickname)\n                                table.insert(myReminders, te